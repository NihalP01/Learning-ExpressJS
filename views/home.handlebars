<h1 class="text-center mb-3">{{title}}</h1> 
<form action="/api/students" method="POST" class="mb-4">
  <div class="form-group">
      <label for="id">ID</label>
      <input type="text" name="id" class="form-control">
  </div>
  <div class="form-group">
      <label for="name">Name</label>
      <input type="text" name="name" class="form-control">
  </div>  
  <div class="form-group">
      <label for="Age">Age</label>
      <input type="text" name="age" class="form-control">
  </div> 
  <div class="form-group">
      <label for="rollNo">Roll No</label>
      <input type="text" name="rollNo" class="form-control">
  </div>
  <input type="submit" value="Add Student" class="btn btn-primary btn-block">
</form>
<h4>Students Data</h4>
<ul class="list-group">
    {{#each students}}
     <li class="list-group-item">{{this.id}}. {{this.name}}, Age: {{this.age}}, Roll No: {{this.rollNo}}</li>
    {{/each}} 
</ul>